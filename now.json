{
  "version": 2,
  "alias": "contrast.now.sh",
  "public": true,
  "builds": [
    {
      "src": "www/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    },
    {
      "src": "img/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/img/(?<foreground>[^/]+)/(?<background>[^/]+)",
      "dest": "/img/index.js?foreground=$foreground&background=$background"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ]
}
